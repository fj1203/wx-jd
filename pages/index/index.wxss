.jd-fixed {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}

.jd-header {
  display: flex;
  padding: 10rpx 20rpx;
  background-color: #e43130;
  align-items: center;
  justify-content: space-between;
}
 .jd-search-icon-back {
    width: 1.25rem;
    height: 1rem;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAgCAYAAABgrToAAAAAAXNSR…CbKdwabaaAlw0rmhaDAAIIIIBAQYA2U8CLX2kzFcB8m/kAgGUmQShl8pcAAAAASUVORK5CYII=) no-repeat;
    /* background-size: contain; */
    vertical-align: top;
    position: relative;
    top: 50%;
    margin-top: -.5rem;
}
.input-wrap {
  border-radius: 50rpx;
  background-color: #fff;
  flex: 1;
  height: 70rpx;
  line-height: 70rpx;
  margin-right: 10rpx;
}

.login {
  color: #fff;
  font-size: 28rpx;
}

.jd-form {
  display: flex;
  height: 70rpx;
  line-height: 70rpx;
}

.search-input {
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
}
.slide{
  padding-top: 70rpx;
}
.class-select {
  padding: 20rpx;
}

.class-item {
  display: flex;
  justify-content: space-between;
}

.jd-class-image {
  text-align: center;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.jd-discount {
  padding-top: 10rpx;
  background-color: #f7f7f7;
}

.jd-discount-bg {
  background-color: #fff;
}

.discount-header {
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  padding-right: 20rpx;
}

.discount-left {
  display: flex;
}

.disc-icon {
  background-image: url(//wq.360buyimg.com/fd/h5/wx/msite/images/sprite@2x_7d630b7f.png);
  background-position: 0 -47px;
  background-size: 143.5px 104px;
  background-repeat: no-repeat;
  width: 89px;
  height: 35px;
  /* position: absolute;
  left: 0; */
  display: inline-flex;
  margin-right: 20rpx;
}

.time-wrap {
  display: flex;
  align-items: center;
}

.time {
  display: inline-flex;
  border: 1px solid #dfdfdf;
  color: #333;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
}

.discount-arrow {
  color: #e93b3d;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

.arrow-text {
  margin-right: 10rpx;
}

.msite-seckill__arrow {
  width: 12px;
  height: 12px;
  display: inline-block;
  background: #e93b3d;
  border-radius: 12px;
  vertical-align: middle;
  margin-top: -2px;
}

.msite-seckill__arrow::before {
  content: "";
  display: block;
  width: 4px;
  height: 4px;
  border-top: 1px solid #fff;
  border-left: 1px solid #fff;
  -webkit-transform-origin: 50%;
  transform-origin: 50%;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
  margin-left: 3px;
  margin-top: 3px;
}

.goods-wrap {
  display: flex;
}

.disc-content-item {
  padding: 0 30rpx;
}

.price {
  text-align: center;
  font-size: 32rpx;
  color: #e93b3d;
  line-height: 40rpx;
  margin-top: 5px;
}

.disc-price {
  font-size: 24rpx;
  text-align: center;
  line-height: 32rpx;
  color: #2c2c2c;
  margin-bottom: 5px;
}

.jd-discount {
  padding-top: 30rpx;
  background-color: #f7f7f7;
}

.jd-discount-bg {
  background-color: #fff;
}

.tuijian {
  padding-top: 10px;
  background-color: #f7f7f7;
}

.msite-rec__title {
  margin: 10px 5px;
  line-height: 16px;
  text-align: center;
  position: relative;
}

.msite-rec__title-line {
  width: 100%;
  position: absolute;
  top: 8px;
}

.msite-rec__title-line::after {
  content: "";
  position: absolute;
  z-index: 1;
  pointer-events: none;
  background-color: #cbcbcb;
  height: 1px;
  left: 0;
  right: 0;
  bottom: 0;
}

.msite-rec__title-con {
  text-align: center;
  display: inline-block;
  background: #f3f5f7;
  padding: 0 10px;
  position: relative;
  z-index: 1;
  font-size: 14px;
  color: #666;
}

.msite-rec__title-icon {
  display: inline-block;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABUAAAAVCAYAAACpF6WWAAAAGXRFW…8xtks/H4RZs8Rl8etybhdNb9QFz7EIP9gud3xN9UZ9CzAA+QmWuz+ZcigAAAAASUVORK5CYII=);
  background-repeat: no-repeat;
  background-size: 13px 13px;
  width: 13px;
  height: 13px;
  margin-right: 5px;
  position: relative;
  vertical-align: middle;
  margin-top: -2px;
}

.msite-rec__goods {
  background-color: #f7f7f7;
}

.msite-rec__goods::after {
  clear: both;
}

.msite-rec__good {
  float: left;
  width: 50%;
  box-sizing: border-box;
  position: relative;
  margin-bottom: 4px;
  color: #333;
}

.msite-rec__good-inner {
  background: #fff;
  position: relative;
  overflow: hidden;
}

.msite-rec__good-desc {
  padding: 0 5px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 18px;
  height: 36px;
  margin: 3px 0;
  font-size: 12px;
}

.msite-rec__good-price {
  padding: 0 5px;
  color: #f23030;
  font-size: 16px;
  line-height: 20px;
  height: 20px;
  overflow: hidden;
}

.msite-rec__good-price .small-price {
  font-weight: 400;
  font-size: 12px;
}

.msite-rec__good-tip {
  font-size: 10px;
  color: #999;
  padding: 0 5px;
  line-height: 20px;
  height: 20px;
  margin-top: 4px;
  margin-bottom: 5px;
}

.msite-rec__good-btn {
  position: absolute;
  right: 5px;
  bottom: 5px;
  width: 50px;
  height: 24px;
  line-height: 24px;
  font-size: 12px;
  text-align: center;
  -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
  border: 1px solid #f7f7f7;
}

.msite-rec__good:nth-child(odd) {
  padding-right: 2px;
}

.msite-rec__good:nth-child(even) {
  padding-left: 2px;
}

.clear {
  clear: both;
}
